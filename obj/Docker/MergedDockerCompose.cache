name: dockercompose347909893981531416
services:
  faceit_stats_provider:
    build:
      context: C:\Users\Tomek\source\repos\Faceit_Stats_Provider
      dockerfile: Faceit_Stats_Provider/Dockerfile
    container_name: Faceit_Stats_Provider
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: faceitstatsprovider
    networks:
      faceit_stats_provider_network: null
    ports:
